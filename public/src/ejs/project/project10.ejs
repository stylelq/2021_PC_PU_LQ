<%
type = "project"
page = "project"
popup = "false"
lnbTitle = "김은학"
%>
<% include ../_include/_header %>
<div class="container">
    <div class="site-project type-lime">
        <!--타이틀-->
        <div class="project-number">
            <div class="project-number__title motion-up">
                <img class="project-number__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_tit.png" alt="05"/>
            </div>
        </div>
        <!--//타이틀-->

        <!--이름 정보-->
        <div class="project-head">
            <div class="project-head__title">
                <img class="project-head__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn1_txt.png" alt=""/>
            </div>
            <div class="project-head__meta">
                <ul class="project-head__list">
                    <!--instagram-->
                    <li class="project-head__item">
                        <a href="https://www.instagram.com/eunhakkim" class="project-head__link instagram" target="_blank">
                            <span class="sr-only">인스타바로가기</span>
                        </a>
                    </li>
                    <!--//instagram-->
                    <!--homepage-->
                    <li class="project-head__item">
                        <a href="https://eunhakkim.com/" class="project-head__link homepage" target="_blank">
                            <span class="sr-only">홈페이지바로가기</span>
                        </a>
                    </li>
                    <!--//homepage-->
                </ul>
            </div>
        </div>
        <!--//이름 정보-->

        <!--정보-->
        <div class="project-info">
            <div class="project-info__title">
                <img class="project-info__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn2_txt1.png" alt=""/>
            </div>
            <div class="project-info__contents motion-up">
                <img class="project-info__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn2_txt2.png" alt=""/>
            </div>
        </div>
        <!--//정보-->

        <!--비디오-->
        <div class="project-video">
            <div class="project-video__wrap">
                <div class="project-video__area is-active nth-1 js-play-btn">
                    <div class="project-video__intro nth-1">
                        <div class="project-video__play"></div>
                    </div>
                    <div class="project-video__youtube" data-url="4-6Oq3xBP3w"></div>
                </div>
            </div>
        </div>
        <!--//비디오-->

        <!--이름-->
        <div class="project-name">
            <div class="project-name__wrap">
                <img class="project-name__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn3_txt1.png" alt=""/>
            </div>
        </div>
        <!--//이름-->

        <!--슬라이드-->
        <div class="project-slide">
            <div class="swiper-container project-slide__container">
                <div class="project-slide__name">
                    <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn4_txt1.png" alt="" />
                </div>
                <div class="swiper-wrapper project-slide__wrapper">
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn4_img1.jpg" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn4_img2.jpg" alt=""/>
                        </div>
                    </div>
                    <div class='swiper-slide project-slide__item'>
                        <div class="project-slide__wrap">
                            <img class="project-slide__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/10/section1_ctn4_img3.jpg" alt=""/>
                        </div>
                    </div>
                </div>
            </div>
            <div class="swiper-pagination project-slide-pagination"></div>
        </div>
        <!--//슬라이드-->

        <!--more-->
        <div class="project-button">
            <div class="project-button__wrap">
                <a href="project.html" class="project-button__link">
                    <img class="project-button__img" src="https://mcdn.louisclub.com/static/mo/img/findintroduce/1/section2_ctn1_btn1.png" alt="" />
                </a>
            </div>
        </div>
        <!--//more-->
    </div>
</div>
<% include ../_include/_contentFooter %>
<% include ../_include/_cartFix %>
<% include ../_include/_footer %>